{
  "extends": "./babel.config.js",
  "presets": ["babel-preset-expo", "mobx", "flow"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@navigation": "./src/navigation",
          "@screens": "./src/screens",
          "@stores": "./src/stores",
          "@utils": "./src/utils",
          "@assets": "./assets",
          "@constants": "./src/constants"
        }
      }
    ]
  ]
}
